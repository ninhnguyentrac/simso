@model SimSoDep.Models.TrietKhauModel
@{
    ViewBag.Title = "AddTrietKhau";
}
<h2>
    AddTrietKhau</h2>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
@using (Html.BeginForm("AddTrietKhau", "DaiLyManager", new { id = 0 }))
{
    @Html.ValidationSummary(true)
    <input type="hidden" name="MaDaiLy" value="@ViewBag.MaDaiLy"/>
    <fieldset>
        <legend>TrietKhauModel</legend>
        <div class="editor-label">
            @Html.LabelFor(model => model.TrietKhau)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.TrietKhau)<span style="font-weight: bold">%</span>
            @Html.ValidationMessageFor(model => model.TrietKhau)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.GiaTu)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.GiaTu)
            @Html.ValidationMessageFor(model => model.GiaTu)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => model.Den)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Den)
            @Html.ValidationMessageFor(model => model.Den)
        </div>
        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}
<div>
    @Html.ActionLink("Back to List", "Edit", new { id = ViewBag.MaDaiLy })
</div>
